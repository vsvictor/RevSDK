<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simpleappintegration">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SimpleAppIntegration"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.nuubit.key"
            android:value="7c3164fc-5c5e-4f8b-92dc-118f0ef50da4" />

        <activity
            android:name="com.simpleappintegration.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.simpleappintegration.ResultActivity"
            android:label="@string/result"
            android:theme="@style/AppTheme.Dialog" />
    </application>

</manifest>
